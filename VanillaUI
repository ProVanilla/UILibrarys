			ButtonIcon_2.Position = UDim2.new(0.908872902, 0, 0.195121944, 0)
			ButtonIcon_2.Size = UDim2.new(0, 25, 0, 25)
			ButtonIcon_2.Image = "rbxassetid://11432848110"
			ButtonIconGradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(255, 205, 89)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(255, 128, 42))}
			ButtonIconGradient.Name = "ButtonIconGradient"
			ButtonIconGradient.Parent = ButtonIcon_2
			PageScrollingFrame.CanvasSize = UDim2.new(0, 0, 0, PageListLayout.AbsoluteContentSize.Y)

			TextButton.MouseEnter:Connect(function()
				TweenService:Create(ButtonIcon_2,TweenInfo.new(0.25),{Rotation = -30}):Play()
				TweenService:Create(ButtonFrame,TweenInfo.new(0.25),{BackgroundTransparency = 0.50}):Play()
			end)
			
			TextButton.MouseLeave:Connect(function()
				TweenService:Create(ButtonIcon_2,TweenInfo.new(0.25),{Rotation = 0}):Play()
				TweenService:Create(ButtonFrame,TweenInfo.new(0.25),{BackgroundTransparency = 2.00}):Play()
			end)

			TextButton.MouseButton1Click:Connect(function()
				pcall(callback)
				TweenService:Create(ButtonIcon_2,TweenInfo.new(0.25),{Rotation = 360}):Play()
				TweenService:Create(ButtonFrame,TweenInfo.new(0.25),{BackgroundTransparency = 0.20}):Play()
				wait(0.25)
				TweenService:Create(ButtonFrame,TweenInfo.new(0.25),{BackgroundTransparency = 0.50}):Play()
			end)
			
			function button_func:Run()
				pcall(callback)
				TweenService:Create(ButtonIcon_2,TweenInfo.new(0.25),{Rotation = 360}):Play()
				TweenService:Create(ButtonFrame,TweenInfo.new(0.25),{BackgroundTransparency = 0.20}):Play()
				wait(0.25)
				TweenService:Create(ButtonFrame,TweenInfo.new(0.25),{BackgroundTransparency = 2.00}):Play()
			end
			return button_func
		end
		
		function uilib_page:MakeToggle(name,bool,callback)
			local ToggleFrame = Instance.new("Frame")
			local ToggleCorner = Instance.new("UICorner")
			local ToggleStroke = Instance.new("UIStroke")
			local ToggleStrokeGradient = Instance.new("UIGradient")
			local ToggleGradient = Instance.new("UIGradient")
			local TextToggleButton = Instance.new("TextButton")
			local TextToggleGradient = Instance.new("UIGradient")
			local ToggleIcon = Instance.new("ImageLabel")
			local ToggleIconGradient = Instance.new("UIGradient")
			local ToggleIconCorner = Instance.new("UICorner")
			local ToggleIconStroke = Instance.new("UIStroke")
			local ToggleIconGradient_2 = Instance.new("UIGradient")
			local callback = callback or function () end
			local ToggleEnabled = bool or false
			local Toggle_func = {}
			
			ToggleFrame.Name = "ToggleFrame"
			ToggleFrame.Parent = PageScrollingFrame
			ToggleFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			ToggleFrame.BackgroundTransparency = 0.900
			ToggleFrame.BorderColor3 = Color3.fromRGB(0, 0, 0)
			ToggleFrame.BorderSizePixel = 0
			ToggleFrame.Position = UDim2.new(0.0345982127, 0, 0.0238907859, 0)
			ToggleFrame.Size = UDim2.new(0, 417, 0, 41)

			ToggleCorner.Name = "ToggleCorner"
			ToggleCorner.Parent = ToggleFrame

			ToggleStroke.Color = Color3.fromRGB(255, 255, 255)
			ToggleStroke.Name = "ToggleStroke"
			ToggleStroke.Parent = ToggleFrame

			ToggleStrokeGradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(255, 175, 137)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(252, 252, 252))}
			ToggleStrokeGradient.Name = "ToggleStrokeGradient"
			ToggleStrokeGradient.Parent = ToggleStroke

			ToggleGradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(255, 175, 137)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(255, 255, 255))}
			ToggleGradient.Name = "ToggleGradient"
			ToggleGradient.Parent = ToggleFrame

			TextToggleButton.Name = "TextToggleButton"
			TextToggleButton.Parent = ToggleFrame
			TextToggleButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			TextToggleButton.BackgroundTransparency = 2.000
			TextToggleButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
			TextToggleButton.BorderSizePixel = 0
			TextToggleButton.Position = UDim2.new(0.0311750602, 0, 0, 0)
			TextToggleButton.Size = UDim2.new(0, 404, 0, 41)
			TextToggleButton.Font = Enum.Font.GothamBold
			TextToggleButton.Text = name
			TextToggleButton.TextColor3 = Color3.fromRGB(255, 255, 255)
			TextToggleButton.TextSize = 14.000
			TextToggleButton.TextXAlignment = Enum.TextXAlignment.Left

			TextToggleGradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(255, 205, 89)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(255, 128, 42))}
			TextToggleGradient.Name = "TextToggleGradient"
			TextToggleGradient.Parent = TextToggleButton

			ToggleIcon.Name = "ToggleIcon"
			ToggleIcon.Parent = ToggleFrame
			ToggleIcon.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			ToggleIcon.BackgroundTransparency = 2.000
			ToggleIcon.BorderColor3 = Color3.fromRGB(0, 0, 0)
			ToggleIcon.BorderSizePixel = 0
			ToggleIcon.Position = UDim2.new(0.908872902, 0, 0.195121944, 0)
			ToggleIcon.Size = UDim2.new(0, 25, 0, 25)
			ToggleIcon.ImageColor3 = Color3.fromRGB(207, 190, 6)
			
			ToggleIconGradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(255, 205, 89)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(255, 128, 42))}
			ToggleIconGradient.Name = "ToggleIconGradient"
			ToggleIconGradient.Parent = ToggleIcon
			ToggleIconCorner.CornerRadius = UDim.new(0, 5)
			ToggleIconCorner.Name = "ToggleIconCorner"
			ToggleIconCorner.Parent = ToggleIcon

			ToggleIconStroke.Color = Color3.fromRGB(255, 255, 255)
			ToggleIconStroke.Name = "ToggleIconStroke"
			ToggleIconStroke.Parent = ToggleIcon

			ToggleIconGradient_2.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(255, 205, 89)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(255, 128, 42))}
			ToggleIconGradient_2.Name = "ToggleIconGradient"
			ToggleIconGradient_2.Parent = ToggleIconStroke
			PageScrollingFrame.CanvasSize = UDim2.new(0, 0, 0, PageListLayout.AbsoluteContentSize.Y)

			ToggleFrame.MouseEnter:Connect(function()
				TweenService:Create(ToggleIconGradient,TweenInfo.new(0.25),{Rotation = 10}):Play()
				ToggleIcon:TweenSize(UDim2.new(0, 30, 0, 30),"InOut","Quad",0.25,true)
				TweenService:Create(ToggleFrame,TweenInfo.new(0.25),{BackgroundTransparency = 0.50}):Play()
			end)

			ToggleFrame.MouseLeave:Connect(function()
				TweenService:Create(ToggleIconGradient,TweenInfo.new(0.25),{Rotation = 0}):Play()
				ToggleIcon:TweenSize(UDim2.new(0, 25, 0, 25),"InOut","Quad",0.25,true)
				TweenService:Create(ToggleFrame,TweenInfo.new(0.25),{BackgroundTransparency = 2.00}):Play()
			end)

			TextToggleButton.MouseButton1Click:Connect(function()
				ToggleEnabled = not ToggleEnabled
				if ToggleEnabled then
					TweenService:Create(ToggleIcon,TweenInfo.new(0.25),{BackgroundTransparency = 0.000}):Play()
				else
					TweenService:Create(ToggleIcon,TweenInfo.new(0.25),{BackgroundTransparency = 2.000}):Play()
				end
				pcall(callback,ToggleEnabled)
			end)
			
			function Toggle_func:Set(bool)
				ToggleEnabled = bool
				if ToggleEnabled then
					TweenService:Create(ToggleIcon,TweenInfo.new(0.25),{BackgroundTransparency = 0.000}):Play()
				else
					TweenService:Create(ToggleIcon,TweenInfo.new(0.25),{BackgroundTransparency = 2.000}):Play()
				end
				pcall(callback,ToggleEnabled)
				
			end
			return Toggle_func
		end
		
		function uilib_page:MakeTextbox(name,txt,callback)
			local TextBoxFrame = Instance.new("Frame")
			local TextBoxFrameCorner = Instance.new("UICorner")
			local TextBoxFrameStroke = Instance.new("UIStroke")
			local TextBoxFrameStrokeGradient = Instance.new("UIGradient")
			local TextBoxFrameGradient = Instance.new("UIGradient")
			local TextBoxLabel = Instance.new("TextLabel")
			local TextBoxLabelGradient = Instance.new("UIGradient")
			local TextBox = Instance.new("TextBox")
			local TextBoxStroke = Instance.new("UIStroke")
			local TextBoxStrokeGradient = Instance.new("UIGradient")
			local TextBoxCorner = Instance.new("UICorner")
			local callback = callback or function () end
			
			TextBoxFrame.Name = "TextBoxFrame"
			TextBoxFrame.Parent = PageScrollingFrame
			TextBoxFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
