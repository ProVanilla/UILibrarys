			TextBox.Parent = TextBoxFrame
			TextBox.BackgroundColor3 = Color3.fromRGB(255,255,255)
			TextBox.BorderColor3 = Color3.fromRGB(0, 0, 0)
			TextBox.BorderSizePixel = 0
			TextBox.Position = UDim2.new(0.465227813, 0, 0.122592859, 0)
			TextBox.Size = UDim2.new(0, 200, 0, 42)
			TextBox.Font = Enum.Font.GothamBold
			TextBox.PlaceholderColor3 = Color3.fromRGB(255, 255, 255)
			TextBox.Text = txt
			TextBox.TextColor3 = Color3.fromRGB(218, 193, 95)
			TextBox.TextSize = 16.000

			TextBoxStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
			TextBoxStroke.Color = Color3.fromRGB(255, 255, 255)
			TextBoxStroke.Name = "TextBoxStroke"
			TextBoxStroke.Parent = TextBox

			TextBoxStrokeGradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(255, 175, 137)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(252, 252, 252))}
			TextBoxStrokeGradient.Name = "TextBoxStrokeGradient"
			TextBoxStrokeGradient.Parent = TextBoxStroke

			TextBoxCorner.CornerRadius = UDim.new(0, 6)
			TextBoxCorner.Name = "TextBoxCorner"
			TextBoxCorner.Parent = TextBox
			PageScrollingFrame.CanvasSize = UDim2.new(0, 0, 0, PageListLayout.AbsoluteContentSize.Y)


			TextBox.Focused:Connect(function()
				TweenService:Create(TextBox,TweenInfo.new(0.25),{TextSize = 20}):Play()
				TweenService:Create(TextBoxFrame,TweenInfo.new(0.25),{BackgroundTransparency = 0.70}):Play()
			end)
			
			TextBox.FocusLost:Connect(function()
				TweenService:Create(TextBox,TweenInfo.new(0.10),{TextSize = 16}):Play()
				TweenService:Create(TextBoxFrame,TweenInfo.new(0.10),{BackgroundTransparency = 2}):Play()
				callback(TextBox.Text)
				TweenService:Create(TextBox,TweenInfo.new(0.10),{TextTransparency = 2}):Play()
				wait(0.10)
				TextBox.Text = txt
				TweenService:Create(TextBox,TweenInfo.new(0.10),{TextTransparency = 0}):Play()
			end)

		end
		
		function uilib_page:MakeSlider(name,min,max,start,callback)
			local SliderFunc = {}
			local dragging = false

			local SliderFrame = Instance.new("Frame")
			local SliderFrameCorner = Instance.new("UICorner")
			local SliderFrameStroke = Instance.new("UIStroke")
			local TextBoxFrameStrokeGradient_2 = Instance.new("UIGradient")
			local SliderFrameGradient = Instance.new("UIGradient")
			local SliderFrameLabel = Instance.new("TextLabel")
			local TextBoxLabelGradient_2 = Instance.new("UIGradient")
			local SliderButton = Instance.new("TextButton")
			local SliderFrameStroke_2 = Instance.new("UIStroke")
			local TextBoxFrameStrokeGradient_3 = Instance.new("UIGradient")
			local SliderFrameCorner_2 = Instance.new("UICorner")
			local SliderLine = Instance.new("Frame")
			local SliderFrameCorner_3 = Instance.new("UICorner")
			local UIGradient = Instance.new("UIGradient")
			local SliderNumber = Instance.new("TextLabel")
			local TextBoxLabelGradient_4 = Instance.new("UIGradient")

			SliderFrame.Name = "SliderFrame"
			SliderFrame.Parent = PageScrollingFrame
			SliderFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			SliderFrame.BackgroundTransparency = 0.900
			SliderFrame.BorderColor3 = Color3.fromRGB(0, 0, 0)
			SliderFrame.BorderSizePixel = 0
			SliderFrame.Position = UDim2.new(0.0345982127, 0, 0.351535827, 0)
			SliderFrame.Size = UDim2.new(0, 417, 0, 57)

			SliderFrameCorner.Name = "SliderFrameCorner"
			SliderFrameCorner.Parent = SliderFrame

			SliderFrameStroke.Color = Color3.fromRGB(255, 255, 255)
			SliderFrameStroke.Name = "SliderFrameStroke"
			SliderFrameStroke.Parent = SliderFrame

			TextBoxFrameStrokeGradient_2.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(255, 175, 137)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(252, 252, 252))}
			TextBoxFrameStrokeGradient_2.Name = "TextBoxFrameStrokeGradient"
			TextBoxFrameStrokeGradient_2.Parent = SliderFrameStroke

			SliderFrameGradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(255, 175, 137)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(255, 255, 255))}
			SliderFrameGradient.Name = "SliderFrameGradient"
			SliderFrameGradient.Parent = SliderFrame

			SliderFrameLabel.Name = "SliderFrameLabel"
			SliderFrameLabel.Parent = SliderFrame
			SliderFrameLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			SliderFrameLabel.BackgroundTransparency = 2.000
			SliderFrameLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
			SliderFrameLabel.BorderSizePixel = 0
			SliderFrameLabel.Position = UDim2.new(0.0311750602, 0, 0.122592859, 0)
			SliderFrameLabel.Size = UDim2.new(0, 391, 0, 22)
			SliderFrameLabel.Font = Enum.Font.GothamBold
			SliderFrameLabel.Text = "Slider "
			SliderFrameLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
			SliderFrameLabel.TextSize = 14.000
			SliderFrameLabel.TextXAlignment = Enum.TextXAlignment.Left

			TextBoxLabelGradient_2.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(255, 205, 89)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(255, 128, 42))}
			TextBoxLabelGradient_2.Name = "TextBoxLabelGradient"
			TextBoxLabelGradient_2.Parent = SliderFrameLabel

			SliderButton.Name = "SliderButton"
			SliderButton.Parent = SliderFrame
			SliderButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			SliderButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
			SliderButton.BorderSizePixel = 0
			SliderButton.Position = UDim2.new(0.0311750975, 0, 0.61403507, 0)
			SliderButton.Size = UDim2.new(0, 390, 0, 14)
			SliderButton.AutoButtonColor = false
			SliderButton.Font = Enum.Font.SourceSans
			SliderButton.Text = ""
			SliderButton.TextColor3 = Color3.fromRGB(0, 0, 0)
			SliderButton.TextSize = 14.000

			SliderFrameStroke_2.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
			SliderFrameStroke_2.Color = Color3.fromRGB(255, 255, 255)
			SliderFrameStroke_2.Name = "SliderFrameStroke"
			SliderFrameStroke_2.Parent = SliderButton

			TextBoxFrameStrokeGradient_3.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(255, 175, 137)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(252, 252, 252))}
			TextBoxFrameStrokeGradient_3.Name = "TextBoxFrameStrokeGradient"
			TextBoxFrameStrokeGradient_3.Parent = SliderFrameStroke_2

			SliderFrameCorner_2.Name = "SliderFrameCorner"
			SliderFrameCorner_2.Parent = SliderButton

			SliderLine.Name = "SliderLine"
			SliderLine.Parent = SliderButton
			SliderLine.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			SliderLine.BorderColor3 = Color3.fromRGB(0, 0, 0)
			SliderLine.BorderSizePixel = 0
			SliderLine.Size = UDim2.new((start or 0)/max, -6,0, 14)


			SliderFrameCorner_3.Name = "SliderFrameCorner"
			SliderFrameCorner_3.Parent = SliderLine

			UIGradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(255, 205, 89)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(255, 128, 42))}
			UIGradient.Parent = SliderLine


			SliderNumber.Name = "SliderNumber"
			SliderNumber.Parent = SliderFrame
			SliderNumber.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			SliderNumber.BackgroundTransparency = 2.000
			SliderNumber.BorderColor3 = Color3.fromRGB(0, 0, 0)
			SliderNumber.BorderSizePixel = 0
			SliderNumber.Position = UDim2.new(0.853717029, 0, 0.122592859, 0)
			SliderNumber.Size = UDim2.new(0, 47, 0, 22)
			SliderNumber.Font = Enum.Font.GothamBold
			SliderNumber.Text = tostring(start and math.floor((start / max) * (max - min) + min) or 0)

			SliderNumber.TextColor3 = Color3.fromRGB(255, 255, 255)
			SliderNumber.TextSize = 14.000
			SliderNumber.TextTransparency = 2

			TextBoxLabelGradient_4.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(255, 205, 89)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(255, 128, 42))}
			TextBoxLabelGradient_4.Name = "adddadad"
			TextBoxLabelGradient_4.Parent = SliderNumber
			
			PageScrollingFrame.CanvasSize = UDim2.new(0, 0, 0, PageListLayout.AbsoluteContentSize.Y)

			local function move(input)
				local pos =UDim2.new(math.clamp((input.Position.X - SliderFrame.AbsolutePosition.X) / SliderFrame.AbsoluteSize.X, 0, 1),-6,-0.644999981,0)
				local pos1 =UDim2.new(math.clamp((input.Position.X - SliderFrame.AbsolutePosition.X) / SliderFrame.AbsoluteSize.X, 0, 1),0,0,14)
				TweenService:Create(SliderLine,TweenInfo.new(0.25),{Size = pos1}):Play()

				local value = math.floor(((pos.X.Scale * max) / max) * (max - min) + min)
				SliderNumber.Text = tostring(value)
				pcall(callback, value)
			end
			
			SliderButton.InputBegan:Connect(function(input)
				if input.UserInputType == Enum.UserInputType.MouseButton1 then
					dragging = true
					TweenService:Create(SliderNumber,TweenInfo.new(0.25),{TextTransparency = 0}):Play()
				end
			end)
			
			SliderButton.InputEnded:Connect(function(input)
				if input.UserInputType == Enum.UserInputType.MouseButton1 then
					dragging = false
					TweenService:Create(SliderNumber,TweenInfo.new(0.25),{TextTransparency = 2}):Play()
				end
			end)
			
			game:GetService("UserInputService").InputChanged:Connect(function(input)
				if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
					move(input)
				end
			end)
			
			pcall(callback, SliderNumber.Text)

			function SliderFunc:Change(tochange)
				if tonumber(tochange) > max then
					tochange = max
					local pos = UDim2.new((tochange or 0) / max, 0, 0, 14)
					TweenService:Create(SliderLine,TweenInfo.new(0.25),{Size = pos}):Play()
					SliderNumber.Text = tostring(tochange)
					TweenService:Create(SliderNumber,TweenInfo.new(0.25),{TextTransparency = 0}):Play()
					wait(0.50)
					TweenService:Create(SliderNumber,TweenInfo.new(0.25),{TextTransparency = 2}):Play()
					pcall(callback, tochange)
				elseif tonumber(tochange) < min then
					tochange = min
					TweenService:Create(SliderLine,TweenInfo.new(0.25),{Size = UDim2.new(0,0,0,14)}):Play()
					SliderNumber.Text = tostring(tochange)
					TweenService:Create(SliderNumber,TweenInfo.new(0.25),{TextTransparency = 0}):Play()
					wait(0.50)
					TweenService:Create(SliderNumber,TweenInfo.new(0.25),{TextTransparency = 2}):Play()
					pcall(callback, tochange)
				else
					local pos = UDim2.new((tochange or 0) / max, 0, 0, 14)
					TweenService:Create(SliderLine,TweenInfo.new(0.25),{Size = pos}):Play()
					SliderNumber.Text = tostring(tochange)
					TweenService:Create(SliderNumber,TweenInfo.new(0.25),{TextTransparency = 0}):Play()
					wait(0.50)
					TweenService:Create(SliderNumber,TweenInfo.new(0.25),{TextTransparency = 2}):Play()
					pcall(callback, tochange)
				end
			end
			return SliderFunc
		end
		return uilib_page
	end
	return uilib_tab
end
