	local uilib_tab = {}
	function uilib_tab:MakeTab(name,icon)
		local PageScrollingFrame = Instance.new("ScrollingFrame")
		local PageListLayout = Instance.new("UIListLayout")
		local FixFrame = Instance.new("Frame")
		local TabButton = Instance.new("TextButton")
		local TabButtonGradient = Instance.new("UIGradient")
		local TabButtonCorner = Instance.new("UICorner")
		local ButtonIcon = Instance.new("ImageLabel")
		
		PageScrollingFrame.Name = name.."_Page"
		PageScrollingFrame.Parent = Folder
		PageScrollingFrame.Active = true
		PageScrollingFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		PageScrollingFrame.BackgroundTransparency = 2.000
		PageScrollingFrame.BorderColor3 = Color3.fromRGB(0, 0, 0)
		PageScrollingFrame.BorderSizePixel = 0
		PageScrollingFrame.Visible = true
		PageScrollingFrame.Position = UDim2.new(0, 0, 0, 0)
		PageScrollingFrame.Size = UDim2.new(0, 458,0, 260)
		PageScrollingFrame.ScrollBarImageColor3 = Color3.fromRGB(255, 252, 161)
		PageScrollingFrame.ScrollBarThickness = 5
		PageScrollingFrame.CanvasSize = UDim2.new(0, 0, 0, 0)
		
		local UIPadding = Instance.new("UIPadding")

		UIPadding.Parent = PageScrollingFrame
		UIPadding.PaddingLeft = UDim.new(0, 20)
		UIPadding.PaddingTop = UDim.new(0, 0)

		PageListLayout.Name = "PageListLayout"
		PageListLayout.Parent = PageScrollingFrame
		PageListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
		PageListLayout.SortOrder = Enum.SortOrder.LayoutOrder
		PageListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Left
		PageListLayout.VerticalAlignment = Enum.VerticalAlignment.Top
		PageListLayout.Padding = UDim.new(0, 10)

		FixFrame.Name = "FixFrame"
		FixFrame.Parent = PageScrollingFrame
		FixFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		FixFrame.BorderColor3 = Color3.fromRGB(0, 0, 0)
		FixFrame.BorderSizePixel = 0
		FixFrame.Position = UDim2.new(0.015625, 0, 0, 0)
		FixFrame.Size = UDim2.new(0, 434, 0, 0)
		TabsScrollingFrame.CanvasSize = UDim2.new(0, 0, 0, UIListLayout.AbsoluteContentSize.Y)
		
		TabButton.Name = name.."Server"
		TabButton.Parent = TabsScrollingFrame
		TabButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		TabButton.BackgroundTransparency = 2
		
		TabButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
		TabButton.BorderSizePixel = 0
		TabButton.Position = UDim2.new(0.100000001, 0, 0, 0)
		TabButton.Size = UDim2.new(0, 35, 0, 35)
		TabButton.AutoButtonColor = false
		TabButton.Font = Enum.Font.SourceSans
		TabButton.Text = ""
		TabButton.TextTransparency = 2.00
		TabButton.TextColor3 = Color3.fromRGB(0, 0, 0)
		TabButton.TextSize = 14.000

		TabButtonGradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(255, 205, 89)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(255, 128, 42))}
		TabButtonGradient.Name = "TabButtonGradient"
		TabButtonGradient.Parent = TabButton

		TabButtonCorner.CornerRadius = UDim.new(0, 5)
		TabButtonCorner.Name = "TabButtonCorner"
		TabButtonCorner.Parent = TabButton

		ButtonIcon.Name = "ButtonIcon"
		ButtonIcon.Parent = TabButton
		ButtonIcon.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		ButtonIcon.BackgroundTransparency = 2.000
		ButtonIcon.BorderColor3 = Color3.fromRGB(0, 0, 0)
		ButtonIcon.BorderSizePixel = 0
		ButtonIcon.Size = UDim2.new(0, 35, 0, 35)
		ButtonIcon.Image = "rbxassetid://"..icon
		ButtonIcon.ImageTransparency = 2
		
		
		TabButton.MouseButton1Click:Connect(function()
			if TabName.Text == "• "..name then
				return
			else
				for i,v in next, TabsScrollingFrame:GetChildren() do
					if v:IsA("TextButton") then
						v:TweenSize(UDim2.new(0, 35, 0, 35),"In","Quad",0.1,true)
						v.ButtonIcon:TweenSize(UDim2.new(0, 35, 0, 35),"In","Quad",0.1,true)
					end
					TabButton:TweenSize(UDim2.new(0, 40, 0, 40),"In","Quad",0.1,true)
					ButtonIcon:TweenSize(UDim2.new(0, 40, 0, 40),"In","Quad",0.1,true)

				end

				for i,v in next, Folder:GetChildren() do
					currentpage = string.gsub(TabButton.Name,"Server","").."_Page"
					if v.Name == currentpage then
						UIPageLayout:JumpTo(v)
					end
				end
				TweenService:Create(TabName,TweenInfo.new(0.4),{TextTransparency = 2.00}):Play()
				wait(0.4)
				TabName.Text = "• "..name
				TweenService:Create(TabName,TweenInfo.new(0.4),{TextTransparency = 0.00}):Play()
			end
		end)
		
		for i,v in next, TabsScrollingFrame:GetChildren() do
			if v:IsA("TextButton") then
				TweenService:Create(v.ButtonIcon,TweenInfo.new(0.4),{ImageTransparency = 0}):Play()
				TweenService:Create(v,TweenInfo.new(0.4),{BackgroundTransparency = 0}):Play()
			end
		end

		if abc == false then
			for i,v in next, TabsScrollingFrame:GetChildren() do
				if v:IsA("TextButton") then
					v:TweenSize(UDim2.new(0, 35, 0, 35),"In","Quad",0.1,true)
					v.ButtonIcon:TweenSize(UDim2.new(0, 35, 0, 35),"In","Quad",0.1,true)
					ButtonIcon:TweenSize(UDim2.new(0, 40, 0, 40),"In","Quad",0.1,true)
					TabButton:TweenSize(UDim2.new(0, 40, 0, 40),"In","Quad",0.1,true)
				end
				wait(0.4)
				UIPageLayout:JumpToIndex(1)
				TabName.Text = "• "..name
				TweenService:Create(TabName,TweenInfo.new(0.4),{TextTransparency = 0.00}):Play()
			end
			abc = true
		end
		
		game:GetService("RunService").Stepped:Connect(function()
			pcall(function()
				PageScrollingFrame.CanvasSize = UDim2.new(0,0,0,PageListLayout.AbsoluteContentSize.Y + 20)
				TabsScrollingFrame.CanvasSize = UDim2.new(0,0,0,UIListLayout.AbsoluteContentSize.Y + 20)
			end)
		end)

		local uilib_page = {}
